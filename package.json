{
    "name": "@8ctavio/resolve-module-export-names",
    "version": "0.0.1",
    "license": "MIT",
    "description": "Retrieve names of JavaScript modules' exported values",
    "author": "Octavio Araiza (https://github.com/8ctavio)",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/8ctavio/resolve-module-export-names.git"
    },
    "bugs": "https://github.com/8ctavio/resolve-module-export-names/issues",
    "type": "module",
    "scripts": {
        "lint": "oxlint",
        "test": "vitest run",
        "typecheck": "tsc -p ./tsconfig.lib.json --noEmit",
        "dts": "tsc -b ./tsconfig.lib.json"
    },
    "files": ["src", "dist-dts"],
    "exports": {
        ".": {
            "types": "./src/dist-dts/index.d.ts",
            "import": "./src/index.js"
        }
    },
    "dependencies": {
        "oxc-parser": "catalog:consumer",
        "oxc-resolver": "catalog:consumer"
    },
    "devDependencies": {
        "@types/node": "catalog:",
        "oxlint": "catalog:",
        "typescript": "catalog:",
        "vitest": "catalog:"
    },
    "devEngines": {
        "runtime": [
            {
                "name": "node",
                "version": "^22.17.1",
                "onFail": "warn"
            }
        ]
    },
    "packageManager": "pnpm@10.15.1"
}