{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"module": "node20",
		"moduleResolution": "nodenext",
		"moduleDetection": "force",
		"target": "esnext",
		"erasableSyntaxOnly": true,
		"verbatimModuleSyntax": true,
		"strict": true,
		"noImplicitOverride": true,
		"noUncheckedIndexedAccess": true
	}
}